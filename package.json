{
  "name": "@f3liz/rescript-autogen-openapi",
  "version": "0.6.0",
  "description": "Generate ReScript code with Sury schemas from OpenAPI 3.1 specs. Supports multiple forks with diff/merge capabilities.",
  "keywords": [
    "rescript",
    "openapi",
    "codegen",
    "sury",
    "schema",
    "misskey",
    "api",
    "generator"
  ],
  "author": "NyanRus",
  "license": "MPL-2.0",
  "files": [
    "src/**/*.res",
    "lib/es6/src/**/*.mjs",
    "lib/es6/src/**/*.d.ts",
    "rescript.json",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "url": "https://github.com/f3liz-dev/rescript-autogen-openapi"
  },
  "scripts": {
    "build": "rescript && node scripts/generate-dts.mjs",
    "clean": "rescript clean",
    "watch": "rescript build -w",
    "test": "node --test",
    "prepublishOnly": "npm run build && npm test"
  },
  "main": "lib/es6/src/Codegen.mjs",
  "types": "lib/es6/src/Codegen.d.ts",
  "type": "module",
  "dependencies": {
    "@readme/openapi-parser": "^5.5.0",
    "handlebars": "^4.7.8",
    "js-convert-case": "^4.2.0",
    "pathe": "^2.0.3",
    "toposort": "^2.0.2"
  },
  "devDependencies": {
    "rescript": "^12.1.0",
    "sury": "11.0.0-alpha.4",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "rescript": "^12.0.0",
    "sury": ">=11.0.0-alpha.4"
  }
}
